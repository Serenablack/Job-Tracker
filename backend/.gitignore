# Backend specific gitignore

# Sensitive configuration files
client_secret*.json
*service-account*.json
*credentials*.json
jobtracker-*.json
config/secrets.js
config/keys.js

# Environment variables
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Upload directories (contain user data)
uploads/
temp-uploads/
assets/*.xlsx
assets/*.xls
assets/*.pdf

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Logs
logs
*.log

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory
coverage
*.lcov

# Database files
*.db
*.sqlite
*.sqlite3

# Temporary files
tmp/
temp/

# Build output
dist/
build/

# Cache
.cache/
.npm
.eslintcache
